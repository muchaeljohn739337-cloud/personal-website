{
  "name": "saas-platform",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "build:worker": "npm run build && npx @opennextjs/cloudflare",
    "preview:worker": "npx wrangler pages dev .vercel/output/static --compatibility-flags=nodejs_compat",
    "deploy:worker": "npx wrangler pages deploy .vercel/output/static",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "markdown-lint": "markdownlint \"**/*.md\" --ignore node_modules",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:db": "npx tsx scripts/test-db-connection.ts",
    "test:db:setup": "bash scripts/setup-test-database.sh",
    "test:env": "npx tsx scripts/verify-test-env.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:a11y": "npx tsx scripts/check-accessibility.ts",
    "test:a11y:contrast": "npx tsx scripts/check-accessibility.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force && npm run prisma:seed",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "docker:logs": "docker-compose logs -f",
    "prepare": "node -e \"try { require('husky').install() } catch (e) { if (e.code !== 'MODULE_NOT_FOUND') console.log('Husky not installed') }\"",
    "deploy:prod": "npm run build && vercel --prod",
    "deploy:prod:safe": "npx tsx scripts/deploy-to-production.ts",
    "deploy:worker:prod": "npm run build:worker && npx wrangler pages deploy .vercel/output/static --env production",
    "migrate:prod": "prisma migrate deploy",
    "verify:prod": "curl -f https://advanciapayledger.com/api/health || exit 1",
    "post-deploy": "npx tsx scripts/post-deployment-verify.ts",
    "setup:vercel:env:auto": "npx tsx scripts/set-vercel-env-auto.ts",
    "setup:vercel:env": "npx tsx scripts/set-vercel-env-complete.ts",
    "vercel:env:copy": "npx tsx scripts/generate-vercel-env-copy.ts",
    "vercel:env:set": "npx tsx scripts/set-vercel-env-api.ts",
    "preprod:check": "node scripts/pre-production-check.js",
    "security:check": "node scripts/security-check.js",
    "audit:full": "node scripts/full-audit.js",
    "deploy:full": "npm run preprod:check && npm run security:check && npm run deploy:prod",
    "setup:prod": "bash scripts/setup-production.sh",
    "env:template": "node scripts/create-env-template.js",
    "generate:secrets": "node scripts/generate-secrets.js",
    "generate:prod-secrets": "npx tsx scripts/generate-production-secrets.ts",
    "setup:vercel-env": "npx tsx scripts/setup-vercel-env.ts",
    "create-admin": "tsx scripts/create-admin.ts",
    "test:crypto": "tsx scripts/test-crypto-payments.ts",
    "supabase:schema": "npx tsx scripts/supabase-schema-check.ts",
    "supabase:storage": "npx tsx scripts/supabase-storage-check.ts",
    "setup:supabase:env": "npx tsx scripts/setup-supabase-env.ts",
    "test:supabase:auth": "npx tsx scripts/test-supabase-auth.ts",
    "setup:supabase:api:schema": "npx tsx scripts/setup-supabase-api-schema.ts",
    "setup:supabase:auto": "npx tsx scripts/supabase-auto-setup.ts",
    "setup:supabase:complete": "npx tsx scripts/supabase-complete-setup.ts",
    "setup:supabase:buckets": "npx tsx scripts/setup-storage-buckets.ts",
    "setup:deployment": "npx tsx scripts/supabase-vercel-cloudflare-setup.ts",
    "deploy:production": "npx tsx scripts/deploy-production.ts",
    "verify:legitimacy": "tsx scripts/verify-legitimacy.ts",
    "health:check": "tsx -e \"import('./lib/self-healing/system').then(m => m.runHealthCheck().then(h => console.log(JSON.stringify(h, null, 2))))\"",
    "check:production": "npx tsx scripts/check-production-errors.ts",
    "verify:vercel:env": "npx tsx scripts/verify-vercel-env.ts",
    "generate:prod:secrets": "npx tsx scripts/generate-production-secrets.ts",
    "deploy:checklist": "npx tsx scripts/deployment-master-checklist.ts",
    "deploy:vercel:script": "bash scripts/deployment/deploy-vercel.sh",
    "deploy:cloudflare:script": "bash scripts/deployment/deploy-cloudflare.sh",
    "worker:start": "npx tsx scripts/start-agent-worker.ts",
    "worker:test": "npx tsx scripts/test-agent-system.ts",
    "worker:verify": "npx tsx scripts/verify-worker-setup.ts",
    "worker:check-db": "npx tsx scripts/check-database.ts",
    "worker:demo": "npx tsx scripts/demo-agent-worker.ts",
    "setup:notes-table": "npx tsx scripts/setup-notes-table.ts",
    "verify:startup": "npx tsx scripts/verify-startup.ts",
    "check:admin": "npx tsx scripts/check-admin-exists.ts",
    "test:admin-login": "npx tsx scripts/test-admin-login.ts"
  },

{
  "name": "saas-platform",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "build:worker": "npx @cloudflare/next-on-pages",
    "preview:worker": "npx wrangler pages dev .vercel/output/static --compatibility-flags=nodejs_compat",
    "deploy:worker": "npx wrangler pages deploy .vercel/output/static",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "markdown-lint": "markdownlint \"**/*.md\" --ignore node_modules",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force && npm run prisma:seed",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "docker:logs": "docker-compose logs -f",
    "prepare": "husky install"
  },
